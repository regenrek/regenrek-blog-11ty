{
  "name": "regenrek-blog-11ty",
  "version": "1.0.1",
  "description": "My personal regenrek.at Blog",
  "scripts": {
    "build": "npx eleventy",
    "now-build": "postcss assets/css/style.pcss -o dist/css/style.min.css && npx eleventy",
    "watch": "npx eleventy --watch",
    "dev": "npx eleventy --serve",
    "build-css": "postcss assets/css/style.pcss -o  dist/css/style.min.css -w",
    "debug": "DEBUG=* npx eleventy"
  },
