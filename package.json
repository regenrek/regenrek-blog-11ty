    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.42.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
