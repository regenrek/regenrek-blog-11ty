  },
  "homepage": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate",
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "autoprefixer": "^9.5.0",
    "cssnano": "^4.1.10",
    "luxon": "^1.11.0",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^4.0.0",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "postcss-cssnext": "^3.1.0",
