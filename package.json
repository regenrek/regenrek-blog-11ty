{
  "name": "regenrek-blog-11ty",
  "version": "2.0.0",
  "description": "regenrek.com - Minimalistic Eleventy (11ty) blog with webpack, PostCSS and Tailwind",
  "scripts": {
    "build": "npm-run-all clean build:assets build:site purge",
    "build:assets": "NODE_ENV=production webpack --mode=production",
    "build:site": "NODE_ENV=production eleventy",
    "clean": "rm -rf ./dist",
    "dev": "npm-run-all clean webpack:assets --parallel dev:*",
    "dev:assets": "yarn run webpack:assets --watch",
    "dev:site": "NODE_ENV=development eleventy --serve",
    "webpack:assets": "NODE_ENV=development webpack --mode=development",
    "purge": "purgecss --config purgecss.config.js --output dist/assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regenrek/regenrek-blog-11ty"
  },
  "author": {
    "name": "Kevin Regenrek",
    "email": "hello@regenrek.at",
    "url": "https://www.regenrek.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "hhttps://github.com/regenrek/regenrek-blog-11ty/issues"
  },
  "homepage": "https://github.com/regenrek/regenrek-blog-11ty",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.5",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eleventy-plugin-nesting-toc": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "file-loader": "^3.0.1",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-attrs": "^2.3.2",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-link-attributes": "^2.1.0",
    "markdown-it-link-target": "^1.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
